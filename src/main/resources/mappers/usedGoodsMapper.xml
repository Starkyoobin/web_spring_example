<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.starkyb.spring.lesson02.dao.UsedGoodsDAO">
 	<!-- used_goods 테이블의 모든 행 가져오기 -->
 	<select id="selectUsedGoodsList" resultType="com.starkyb.spring.lesson02.model.UsedGoods">
 		SELECT
 			`id`
 			, `sellerId`
 			, `title`
 			, `price`
 			, `description`
 			, `picture`
 			, `createdAt`
 			, `updatedAt`
 		FROM
 			`used_goods`
 	</select>
 </mapper>